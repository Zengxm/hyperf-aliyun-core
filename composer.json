{
    "name": "ezijing/aliyun-core",
    "description": "AliYun Core For Hyperf",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "aliyun",
        "core"
    ],
    "require": {
        "php": ">=7.2",
        "hyperf/contract": "~2.0.0",
        "hyperf/guzzle": "~2.0.0",
        "hyperf/server": "~2.0.0",
        "psr/container": "~1.0.0"
    },
    "require-dev": {
        "php": ">=7.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wangyizheng",
            "email": "18435134258@163.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ym\\AliyunCore\\": "src/"
        },
        "files": [
            "src/Config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YmTest\\AliyunCore\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Ym\\AliyunCore\\ConfigProvider"
        }
    }
}
